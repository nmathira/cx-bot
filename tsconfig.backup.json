{
  "compilerOptions": {
    "target": "ESNext",
    "removeComments": true,
    "module": "CommonJS",
    "strict": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "outDir": "dist/",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@config/*": ["config/*"]
    }
  },
  "exclude": [
    "dist/",
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    ".gitignore",
    "package-lock.json",
    "package.json",
    "workspace.code-workspace"
  ]
}
